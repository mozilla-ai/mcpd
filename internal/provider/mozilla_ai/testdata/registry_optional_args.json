{
  "time": {
    "id": "time",
    "name": "time",
    "displayName": "Time Server",
    "description": "A server for time and timezone conversions",
    "license": "MIT",
    "categories": ["System Tools"],
    "tags": ["time", "timezone", "date", "converter"],
    "arguments": {
      "--local-timezone": {
        "description": "Override the system's default timezone",
        "example": "America/New_York",
        "name": "--local-timezone",
        "required": false,
        "type": "argument"
      }
    },
    "installations": {
      "uvx": {
        "package": "mcp-server-time",
        "runtime": "uvx",
        "version": "0.2.0",
        "description": "Run with Python uvx",
        "recommended": true
      }
    },
    "tools": [
      {
        "name": "get_current_time",
        "description": "Get the current time in a specific timezone"
      },
      {
        "name": "convert_time",
        "description": "Convert time between timezones"
      }
    ],
    "transports": ["stdio"],
    "isOfficial": true
  },
  "sqlite-with-package": {
    "id": "sqlite-with-package",
    "name": "sqlite-with-package",
    "displayName": "SQLite (Package Field Test)",
    "description": "SQLite server to test package field extraction",
    "license": "MIT",
    "categories": ["Databases"],
    "tags": ["database", "sql"],
    "arguments": {
      "--db-path": {
        "description": "Path to the SQLite database file",
        "example": "~/test.db",
        "name": "--db-path",
        "required": true,
        "type": "argument"
      }
    },
    "installations": {
      "uvx": {
        "package": "mcp-server-sqlite",
        "runtime": "uvx",
        "version": "2025.4.25",
        "description": "Run with Python uvx",
        "recommended": true
      }
    },
    "tools": [
      {
        "name": "read_query",
        "description": "Execute SELECT queries"
      }
    ],
    "transports": ["stdio"],
    "isOfficial": true
  }
}