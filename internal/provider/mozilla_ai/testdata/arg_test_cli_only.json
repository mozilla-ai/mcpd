{
  "cli-only-server": {
    "id": "cli-only-server",
    "name": "cli-only-server",
    "displayName": "CLI Arguments Only Server",
    "description": "Server that uses command-line arguments only",
    "license": "Apache-2.0",
    "arguments": {
      "--output-format": {
        "name": "--output-format",
        "description": "Output format for results",
        "required": true,
        "type": "argument",
        "example": "json"
      },
      "--port": {
        "name": "--port",
        "description": "Port number to listen on",
        "required": false,
        "type": "argument",
        "example": "3000"
      },
      "--enable-cors": {
        "name": "--enable-cors",
        "description": "Enable CORS headers",
        "required": false,
        "type": "argument_bool"
      }
    },
    "installations": {
      "npx": {
        "package": "cli-server",
        "version": "1.5.2",
        "description": "Run with npx",
        "recommended": false,
        "runtime": "npx"
      },
      "uvx": {
        "package": "cli-server",
        "version": "1.5.2",
        "description": "Run with uvx",
        "recommended": true,
        "runtime": "uvx"
      }
    },
    "tools": [
      {
        "name": "format_output",
        "description": "Format output data"
      }
    ],
    "transports": [
      "stdio",
      "http"
    ],
    "isOfficial": false
  }
}
