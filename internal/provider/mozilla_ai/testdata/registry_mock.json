{
  "test-filesystem": {
    "id": "test-filesystem",
    "name": "test-filesystem",
    "displayName": "Test Filesystem Server",
    "description": "Test filesystem server for unit testing",
    "license": "MIT",
    "categories": [
      "tools",
      "filesystem"
    ],
    "tags": [
      "server",
      "filesystem",
      "operations"
    ],
    "arguments": {
      "BASE_PATH": {
        "description": "Base directory for filesystem access",
        "required": true,
        "type": "environment",
        "example": "/Users/username/Documents"
      },
      "READ_ONLY": {
        "description": "Enable read-only mode",
        "required": false,
        "type": "argument_bool"
      }
    },
    "installations": {
      "npx": {
        "args": [
          "--readonly"
        ],
        "package": "test-filesystem-server",
        "env": {
          "BASE_PATH": "${BASE_PATH}"
        },
        "version": "1.0.0",
        "description": "Run with npx",
        "recommended": true,
        "runtime": "npx"
      }
    },
    "tools": [
      {
        "name": "read_file",
        "description": "Read file contents"
      },
      {
        "name": "list_directory",
        "description": "List directory contents"
      }
    ],
    "transports": [
      "stdio"
    ],
    "isOfficial": true
  },
  "test-memory": {
    "id": "test-memory",
    "name": "test-memory",
    "displayName": "Test Memory Server",
    "description": "Test memory server for unit testing",
    "license": "MIT",
    "categories": [
      "tools",
      "memory"
    ],
    "tags": [
      "server",
      "memory",
      "storage"
    ],
    "arguments": {},
    "installations": {
      "uvx": {
        "args": [],
        "package": "test-memory-server",
        "version": "2.0.0",
        "description": "Run with uvx",
        "recommended": true,
        "runtime": "uvx"
      }
    },
    "tools": [
      {
        "name": "store_memory",
        "description": "Store data in memory"
      }
    ],
    "transports": [
      "stdio"
    ],
    "isOfficial": false
  }
}
