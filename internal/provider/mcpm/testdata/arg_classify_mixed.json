{
  "mixed-env-and-flag": {
    "name": "mixed-env-and-flag",
    "display_name": "Mixed Environment and Flag Server",
    "description": "Test server with both env vars and flags using the same variable",
    "license": "MIT",
    "arguments": {
      "DATABASE_URL": {
        "description": "Database connection URL",
        "required": true,
        "example": "postgresql://user:pass@localhost:5432/db"
      }
    },
    "installations": {
      "npx": {
        "type": "npx",
        "command": "npx",
        "args": [
          "test-server",
          "--db-url=${DATABASE_URL}"
        ],
        "env": {
          "DATABASE_URL": "${DATABASE_URL}"
        }
      }
    },
    "tools": [],
    "is_official": false
  }
}